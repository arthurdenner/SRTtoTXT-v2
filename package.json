{
  "name": "srt-to-txt",
  "version": "0.0.1",
  "description": "This is an app built with Electron, made to convert SRT files into DOC files.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ SRTtoDOC win32 --all --out ~/Documents/SRTtoDOC --version 1.4.1 --overwrite --icon=./app/img/favico.icns"
  },
  "author": "Arthur Denner",
  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^8.0.0",
    "electron-prebuilt": "^1.3.6"
  }
}
